<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee
                      http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
    <servlet-name>registration</servlet-name>
    <servlet-class>org.serf.magazineshop.servlets.RegistrationServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>registration</servlet-name>
    <url-pattern>/registration</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>login</servlet-name>
    <servlet-class>org.serf.magazineshop.servlets.LoginServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>login</servlet-name>
    <url-pattern>/login</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>product</servlet-name>
    <servlet-class>org.serf.magazineshop.servlets.ProductController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>product</servlet-name>
    <url-pattern>/product</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>logout</servlet-name>
    <servlet-class>org.serf.magazineshop.servlets.Logout</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>logout</servlet-name>
    <url-pattern>/logout</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>products</servlet-name>
    <servlet-class>org.serf.magazineshop.servlets.ProductsController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>products</servlet-name>
    <url-pattern>/products</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>bucket</servlet-name>
    <servlet-class>org.serf.magazineshop.servlets.BucketController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>bucket</servlet-name>
    <url-pattern>/bucket</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>buckets</servlet-name>
    <servlet-class>org.serf.magazineshop.servlets.BucketsController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>buckets</servlet-name>
    <url-pattern>/buckets</url-pattern>
  </servlet-mapping>
</web-app>
